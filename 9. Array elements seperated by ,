import java.util.*;
public class Array_coma {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner sc = new Scanner(System.in);
		int n = sc.nextInt();
		int[] arr = new int[n];
		for(int i =0;i<n;i++) {
			arr[i] = sc.nextInt();
		}
		for(int i=0;i<n;i++) {
			if(i!=arr.length-1) {
				System.out.print(arr[i]+",");
			}
			if(i==arr.length-1) {
				System.out.print(arr[i]);
			}
		}
	}

}
